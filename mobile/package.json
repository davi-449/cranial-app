    "@types/jest": "^29.5.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "^0.71.5",
    "@types/react-navigation": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "detox": "^20.5.0",
